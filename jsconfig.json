{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
    }
  }
}